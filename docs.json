{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "ToolDesk",
  "colors": {
    "primary": "#00D4FF",
    "light": "#7B2CBF",
    "dark": "#0A1628"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "installation"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "concepts/workspace",
              "concepts/projects",
              "concepts/sandbox"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/creating-projects",
              "guides/frontend-development",
              "guides/backend-development",
              "guides/calling-node-api",
              "guides/sandbox-permissions"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api/node-api",
              "api/shell-api"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "reference/project-structure"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/tooldesk",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Download",
        "href": "https://github.com/tooldesk/releases"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "/quickstart"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "claude",
      "cursor"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/tooldesk"
    }
  }
}
